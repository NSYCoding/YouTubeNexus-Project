{
    "name": "YouTube",
    "id": "NSY.youtubeWebsite",
    "author": "NSY",
    "version": "1.0.0",
    "description": "The YouTube website embedded into Nexus",
    "platforms": ["win32", "linux"],
    "link": "https://github.com/aarontburn/nexus-template-webpage",
    "build": {
        "build-version": 1,
        "excluded": [
            "electron.ts"
        ],
        "included": [],
        "process": "./process/main.js",
        "replace": [
            {
                "from": "{EXPORTED_MODULE_ID}",
                "to": "%id%",
                "at": [
                    "./process/main.ts"
                ]
            },
            {
                "from": "{EXPORTED_MODULE_NAME}",
                "to": "%name%",
                "at": [
                    "./process/main.ts"
                ]
            }
        ]
    }
}